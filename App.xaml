<?xml version = "1.0" encoding = "UTF-8" ?>
<Application xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:classes="clr-namespace:D424.Classes"
             x:Class="D424.App">
    <Application.Resources>
        <ResourceDictionary>
            <classes:InverseBoolConverter x:Key="InverseBoolConverter"/>
            <classes:ColorConverter x:Key="BoolToColorConverter"/>
            <classes:IsEmptyConverter x:Key="IsEmptyConverter"/>
            <classes:IsNotEmptyConverter x:Key="IsNotEmptyConverter"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Styles/Colors.xaml" />
                <ResourceDictionary Source="Resources/Styles/Styles.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
